{
  "name": "Deal Flags Emphasis",
  "description": "Test AI's reliance on deal indicators (featured, great deal, etc.)",
  "config": {
    "ui": {
      "priceFormat": "currency_symbol",
      "dateFormat": "MM/DD/YYYY",
      "urgencyMessages": true,
      "stockCounts": true,
      "showFees": true,
      "buttonText": "Buy Now",
      "currency": "USD"
    },
    "api": {
      "responseFormat": "nested",
      "includeFees": true,
      "includeDealFlags": true,
      "includeDealScore": true,
      "includeValueScore": true,
      "priceField": "price",
      "includeAvailability": true
    },
    "content": {
      "eventDescriptions": "detailed",
      "venueInfo": "full",
      "showReviews": false,
      "showRatings": true
    }
  }
}

